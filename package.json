{
  "name": "luna-pay-api",
  "version": "0.0.3",
  "description": "",
  "author": "",
  "license": "",
  "scripts": {
    "build-docker": "run-s build:*",
    "clean": "rimraf dist",
    "build-local": "run-s clean build:* build:copy:*",
    "build:bundle": "tsc -p tsconfig.build.json",
    "build-run": "run-s build && cd dist && node main.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start-en": "ts-node src/services/helpers/encryption.helper.ts",
    "start:dev": "nodemon",
    "start:prod": "cd dist && node src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "cross-env TS_NODE_FILES=true ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm -- migration:run -d ./src/typeorm/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/typeorm/data-source.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:generate": "npm run typeorm -- migration:generate -d ./src/typeorm/data-source.ts",
    "database:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.10",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.10",
    "@nestjs/platform-express": "^9.3.10",
    "@nestjs/platform-socket.io": "^9.3.10",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.3.10",
    "@types/lodash": "^4.14.191",
    "atob": "^2.1.2",
    "axios": "^1.3.4",
    "axios-logger": "^2.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "exceljs": "^4.3.0",
    "fclone": "^1.0.11",
    "into-stream": "^7.0.0",
    "jimp": "^0.22.7",
    "joi": "^17.8.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.2",
    "node-rsa": "^1.1.1",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "querystring": "^0.2.0",
    "rand-token": "^1.0.1",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.0",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.2",
    "tmp": "^0.2.1",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.0",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.3.10",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.0",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "18.15.3",
    "@types/supertest": "2.0.12",
    "@types/uuid": "^9.0.1",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "jest": "29.5.0",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.4",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tsc-watch": "6.0.0",
    "tsconfig-paths": "4.1.2",
    "tslint": "5.20.1",
    "typescript": "5.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
